{
  "name": "MenuItem",
  "plural": "MenuItems",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "createdAt": "createdOn",
      "updatedAt": "updatedOn",
      "validateUpsert": true,
      "required": false
    },
    "SoftDelete": {
      "deletedAt": "deletedOn"
    }
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "category": {
      "type": "string"
    },
    "price": {
      "type": "number",
      "required": true
    },
    "number": {
      "type": "number"
    }
  },
  "validations": [],
  "relations": {
    "Supplier": {
      "type": "belongsTo",
      "model": "Supplier",
      "foreignKey": "supplierId"
    }
  },
  "acls": [],
  "methods": {}
}
