{
  "name": "aariXaFood",
  "version": "1.0.0",
  "main": "server/server.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "lint": "eslint .",
    "start": "npm run kill && forever -l forever.log -o output.log -e error.log --minUptime 3000 --spinSleepTime 3000 --verbose --uid \"aariXaFood\" --watch server/server.js",
    "posttest": "npm run lint && nsp check",
    "kill": "powershell.exe If (Get-NetTCPConnection -LocalPort 3333) { Stop-Process -Id (Get-NetTCPConnection -LocalPort 3333).OwningProcess -Force }",
    "deploy": "powershell git checkout master; git merge develop; git push; git push live; git checkout develop; pause; npm run kill; npm start"
  },
  "dependencies": {
    "compression": "^1.0.3",
    "cors": "^2.5.2",
    "helmet": "^3.10.0",
    "lodash": "^4.17.10",
    "loopback": "^3.0.0",
    "loopback-boot": "^2.6.5",
    "loopback-component-explorer": "^5.0.0",
    "loopback-ds-timestamp-mixin": "^3.4.1",
    "loopback-softdelete-mixin2": "0.0.11",
    "moment": "^2.22.1",
    "serve-favicon": "^2.0.1",
    "strong-error-handler": "^2.0.0"
  },
  "devDependencies": {
    "eslint": "^3.17.1",
    "eslint-config-loopback": "^8.0.0",
    "forever": "^0.15.3",
    "nsp": "^2.1.0"
  },
  "repository": {
    "type": "",
    "url": ""
  },
